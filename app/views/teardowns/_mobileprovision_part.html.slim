- if @metadata.mobileprovision.present?
  ruby:   
    properties = [
      {
        key: t('teardowns.show.mobileprovision_name'),
        value: "#{@metadata.mobileprovision['profile_name']} (#{release_type_name(@metadata.release_type)})"
      },
      {
        key: t('teardowns.show.uuid'),
        value: @metadata.mobileprovision['uuid']
      },
      {
        key: t('teardowns.show.team_id'),
        value: "#{@metadata.mobileprovision['team_name']} (#{@metadata.mobileprovision['team_identifier'].first})"
      },
      {
        key: t('teardowns.show.created_at'),
        value: l(Time.zone.parse(@metadata.mobileprovision['created_at']), format: :nice)
      },
      {
        key: t('teardowns.show.expired_at'),
        value: expired_date_tips(@metadata.mobileprovision['expired_at'], colorful: true).html_safe
      }
    ]

  .card
    .card-header
      h3.card-title
        - if enable_icon
          i.fa-solid.fa-certificate
        = t('teardowns.show.mobileprovision')
      .card-tools
        button.btn.btn-tool data-lte-toggle="card-collapse"
          i.fa-solid.fa-plus data-lte-icon="expand"
          i.fa-solid.fa-minus data-lte-icon="collapse"
    .card-body
      - properties.each do |property|
        .property
          .property-key = property[:key]
          .property-value = property[:value]
